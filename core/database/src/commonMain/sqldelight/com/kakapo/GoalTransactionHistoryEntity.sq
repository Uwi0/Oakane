CREATE TABLE IF NOT EXISTS GoalTransactionHistoryTable (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    goalId INTEGER NOT NULL,
    dateCreated INTEGER NOT NULL,
    amount REAL NOT NULL,
    walletId INTEGER NOT NULL,
    note TEXT,
    FOREIGN KEY (goalId) REFERENCES GoalTable(id),
    FOREIGN KEY (walletId) REFERENCES WalletTable(id)
);

insertGoalTransaction:
INSERT OR REPLACE INTO GoalTransactionHistoryTable(goalId, dateCreated, amount, walletId, note)
VALUES (?, ?, ?, ?, ?);